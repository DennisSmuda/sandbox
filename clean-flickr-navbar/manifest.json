{
  "name": "clean-flickr-navbar",
  "version": "0.1",
  "manifest_version": 2,
  "description": "Bring back the clean Flickr navbar",
  "author": "Yang Zhang",
  "content_scripts": [{
    "matches": ["http://www.flickr.com/*", "https://www.flickr.com/*"],
    "css": ["inject.css"]
  }],
  "permissions": ["http://www.flickr.com/*", "https://www.flickr.com/*"]
}
