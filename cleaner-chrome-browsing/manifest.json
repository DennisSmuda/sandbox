{
  "name": "cleaner-chrome-browsing",
  "version": "0.1",
  "manifest_version": 2,
  "description": "Make the world cleaner",
  "author": "Yang Zhang",
  "content_scripts": [{
    "matches": ["http://www.flickr.com/*", "https://www.flickr.com/*"],
    "css": ["clean-flickr-navbar.css"]
  }, {
    "matches": ["http://*.gamepedia.com/*", "https://*.gamepedia.com/*"],
    "css": ["clean-gamepedia.css"]
  }, {
    "matches": ["http://*.wikia.com/*", "https://*.wikia.com/*"],
    "css": ["clean-wikia.css"],
    "js": ["clean-wikia.js"],
    "run_at": "document_start"
  }],
  "permissions": [
    "http://*.wikia.com/*",
    "https://*.wikia.com/*",
    "http://*.gamepedia.com/*",
    "https://*.gamepedia.com/*",
    "http://www.flickr.com/*",
    "https://www.flickr.com/*"
  ]
}
